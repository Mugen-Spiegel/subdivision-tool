<p style="color: green"><%= notice %></p>
<div class="container text-center">
  <div class="row">
    <div class="col">
      <div class="card" style="width: 18rem;">
        <div class="card-body">
          <div class="card">
            <%= image_tag("cover-house.webp", :alt => "rss feed") %>
            <div class="card-body">
              <h5 class="card-title"><%= @user.last_name %> Residence</h5>
              <p class="card-text"></p>
            </div>
            <ul class="list-group list-group-flush">
              <li class="list-group-item"><u><%= @user.last_name %>, <%= @user.first_name %> <%= @user.middle_name %></u><h6>Name</h6></li>
              <li class="list-group-item"><u>Block <%=@user&.address&.block%> Lot <%=@user&.address&.lot%> <%=@user&.address&.street%> street </u><h6>Address</h6></li>
            </ul>
            <div class="card-body">
              <%= link_to "Back to users", users_path %>
            </div>
          </div>
        </div>
      </div>
    </div>
    
   <%= render "users/tabs/index"%>
  </div>
</div>