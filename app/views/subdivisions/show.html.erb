<p style="color: green"><%= notice %></p>
<div class="container text-center">
  <h1><%=@subdivision.name%></h1>
  <div class="row">
    <div class="col">
      <div class="card" style="width: 18rem;">
        <div class="card-body">
          <div class="card">
            <%= image_tag("cover-house.webp", :alt => "rss feed") %>
            <div class="card-body">
              <h5 class="card-title"><%= @subdivision.name %> Residence</h5>
              <p class="card-text"></p>
            </div>
            <ul class="list-group list-group-flush">
              <li class="list-group-item"><%= @subdivision.name %></li>
              <li class="list-group-item"><u><%=@subdivision.city%></u><h6>City</h6></li>
              <li class="list-group-item"><u><%=@subdivision.postal_code%></u><h6>Postal Code</h6></li>
            </ul>
            <div class="card-body">
              <%= link_to "Back", subdivisions_path %>
            </div>
          </div>
        </div>
      </div>
    </div>
    
   <%= render "subdivisions/tabs/index"%>
  </div>
</div>